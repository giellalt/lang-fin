SELECT NNom (-1 >>>)(NOT *1 NNom) ;
SELECT NNom + @SUBJ (NOT *-1 @SUBJ BARRIER CLB + COMMA)(NOT *1 @SUBJ BARRIER CLB + COMMA) ;
SELECT NPar + @SUBJ (NOT *-1 @SUBJ BARRIER CLB + COMMA)(NOT *1 @SUBJ BARRIER CLB + COMMA) ;
SELECT NSgNom + @SUBJ (*0 VSg3 + Act + Ind)(NOT *-1 @SUBJ BARRIER CLB + COMMA)(NOT *1 @SUBJ BARRIER CLB + COMMA) ;
SELECT NPlNom + @SUBJ (*0 VPl3 + Act + Ind)(NOT *-1 @SUBJ BARRIER CLB + COMMA)(NOT *1 @SUBJ BARRIER CLB + COMMA) ;
REMOVE Pron + Rel (NOT -1 COMMA) ; # Niin minäkin.

SELECT PxSg3 (*1 HÄN OR ("jokainen") BARRIER CLB) ;
SELECT PxSg3 (*-1 HÄN OR ("jokainen") BARRIER CLB) ;
SELECT PxPl3 (*1 HE BARRIER CLB) ;
SELECT PxPl3 (*-1 HE BARRIER CLB) ;
SELECT PxSg3 (*0 N + Sg)(NOT *-1 N + Pl OR Num + Pl)(NOT *1 N + Pl OR Num + Pl) ;

SELECT ("anteeksi") (-1 COMMA OR >>>) (1 COMMA OR ("!")) ;
SELECT NSgEla (-1 ("kotoisin")) ;
SELECT Prop (1 COLON) ; # + sanoo-like verbs
SELECT ("Matti") + Prop (*1 ("mies") BARRIER Prop) ;
SELECT ("Matti") + Prop (>*1 HÄN BARRIER Prop) ;
SELECT ("Matti") + Prop (>*1 HE) ;
SELECT ("Matti") + Prop (>*1C ("Matti") + Prop) ; # generalize to any prop!
SELECT ("Matti") + Prop (<*-1C ("Matti") + Prop) ;
SELECT ("kello") + CNOUN ;
SELECT Num (*-1 ("kello")) ;

