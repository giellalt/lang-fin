SELECT ConNeg (-1 NEGV) (NOT 0 NEG-AMBIGUOUS) (NOT 1 Neg) (NOT 1 Prs) (0 Prs/Prt)

  # EI HALUTA/=!# EIKä HEITä LöYDY
  # BUT:     EI NIITä VOI MUUTTAA!
  #     EIKä MUUTA PYYDetä KUIN
  # THIS AD HOC WAY IS THE EASIEST.
REMOVE (Pass Neg) (*-1 NEGV LINK *1C PrsPrc LINK NOT *1 CLB?)
# EI VOI JÄÄDÄ
#     (NOT *R Neg) (NOT 0 NEG-AMBIGUOUS) (0 Prs/Prt)
#     (NOT 0 A/N) (NOT *1 Neg) (NOT 1 Nom) (*-1 NEGV LINK NOT *1 CLB?)
#     VAARAllIneN, NEG/N/A-HOMOGRAFIAA ESIINTYY.
#     VRT: EI SUOMALAISTA VOI AINA KAIKESTA SYYTTää.

# Allow selecting of Neg when Conj CS exists. Does not change above example. -- Ryan
REMOVE (Neg) (NOT *-1 NEGV) (NOT *1 NEGV) (NEGATE 0 (Conj CS))

  # IF NO NEGV, THEN NO Neg IN THE SAME CLAUSE.
REMOVE (Neg) (NOT *-1 NEGV) (*1 NEGV LINK *1C Neg)
  # IF NEGV ONLY ON THE RIGHT, AND IT IS ENGAGED, OUT WITH NEG.
REMOVE (Neg) (*-1 NEGV LINK 1C Neg) (NOT -1 CLB?)

# Select Neg if there's a conj reading too.
SELECT (Neg) IF (0 (Conj CS)) (*1 ConNeg BARRIER ConNeg) ;

