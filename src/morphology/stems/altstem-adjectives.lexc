0  A_BACK_SINGULARS    ;
0  A_BACK_PLURALS ;
0  A_BACK_PARTITIVE_A_AN ;
0  A_BACK_ILLATIVE_UN    ;
0  A_BACK_PARTITIVE_JA    ;
0  A_BACK_GENITIVE_JEN    ;
0  A_BACK_ILLATIVE_IHIN   ;
0  A_BACK_COMPARATIVE   ;
0  A_BACK_SUPERLATIVE   ;

LEXICON A_HÄPÄISTY
!! The variationless y stems are basically fronted version of u; they
!! come from bisyllabic adjectives ending in y or the nut participle^s passive
!! in an s stemmed front voweled verb. The traditional dictionary class is
!! € häpäisty häpäisty+A+Sg+Nom defiled
