0  A_BACK_PLURALS ;
0  A_BACK_PARTITIVE_A_AN ;
0  A_BACK_ILLATIVE_EN    ;
0  A_BACK_PARTITIVE_JA    ;
0  A_BACK_GENITIVE_JEN    ;
0  A_BACK_GENITIVE_IN    ;
0  A_BACK_ILLATIVE_IHIN   ;


