!! € hölöinä hölö+A+Pl+Ess
!! € hölöissä hölö+A+Pl+Ine
0  A_FRONT_NOMINATIVE    ;
0  A_FRONT_SINGULARS    ;
0  A_FRONT_PLURALS ;
0  A_FRONT_PARTITIVE_Ä_ÄN ;
0  A_FRONT_ILLATIVE_ÖN    ;
0  A_FRONT_PARTITIVE_JÄ    ;
0  A_FRONT_GENITIVE_JEN    ;
0  A_FRONT_ILLATIVE_IHIN   ;
0  A_FRONT_COMPARATIVE   ;
0  A_FRONT_SUPERLATIVE   ;

